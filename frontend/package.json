{
  "name": "home-organization-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node scripts/critical-css.js",
    "build:no-critical": "tsc && vite build",
    "generate:api": "openapi-typescript ../docs/openapi.json --output src/api/generated/api-types.ts --export-type",
    "generate:api:full": "npx @openapitools/openapi-generator-cli generate -c ../openapitools.json -g typescript-fetch",
    "fetch:schema": "curl -o ../docs/openapi.json http://localhost:8000/openapi.json",
    "build:analyze": "tsc && vite build && npx source-map-explorer dist/assets/*.js --html analyze-report.html",
    "build:visualize": "ANALYZE=true tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:staged": "eslint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "check": "npm run typecheck && npm run lint && npm run format:check",
    "fix": "npm run lint:fix && npm run format",
    "analyze": "npx source-map-explorer dist/assets/*.js",
    "size": "npx vite-bundle-visualizer",
    "critical": "node scripts/critical-css.js",
    "generate-icons": "npx pwa-asset-generator ./public/icons/logo.svg ./public/icons --padding 0 --background '#FAF3E0'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:e2e": "cypress run --e2e",
    "cypress:component": "cypress run --component",
    "e2e": "start-server-and-test 'npm run dev' http://localhost:5178 'npm run cypress:run'",
    "e2e:open": "start-server-and-test 'npm run dev' http://localhost:5178 'npm run cypress'",
    "images:optimize": "node scripts/optimize-images.js ./public ./public",
    "images:optimize:check": "node scripts/optimize-images.js ./public ./public --check",
    "images:generate": "node scripts/generate-room-images.js",
    "prebuild": "npm run images:optimize",
    "prepare": "cd .. && husky install frontend/.husky || true"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fullcalendar/core": "^6.1.11",
    "@fullcalendar/daygrid": "^6.1.11",
    "@fullcalendar/interaction": "^6.1.11",
    "@fullcalendar/list": "^6.1.11",
    "@fullcalendar/react": "^6.1.11",
    "@fullcalendar/timegrid": "^6.1.11",
    "@tanstack/react-query": "^5.90.20",
    "axios": "^1.6.2",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "openapi-typescript": "^6.7.4",
    "@storybook/addon-a11y": "^7.6.10",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-viewport": "^7.6.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^7.6.10",
    "@storybook/react-vite": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "critters": "^0.0.22",
    "cssnano": "^6.0.3",
    "cypress": "^13.6.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-tailwindcss": "^3.14.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "pwa-asset-generator": "^6.3.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sharp": "^0.33.2",
    "source-map-explorer": "^2.5.3",
    "start-server-and-test": "^2.0.3",
    "storybook": "^7.6.10",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-bundle-visualizer": "^1.0.1",
    "vite-imagetools": "^6.2.8",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^0.17.4",
    "workbox-window": "^7.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
